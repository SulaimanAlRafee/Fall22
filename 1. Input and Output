# number1 = 16 # this is a number
# number2 = '17' #this is a string

# we are adding two strings together
number1 = '17'
number2 = '19'
print ("Adding Strings")
print(number1+number2)
print()

# we are adding two numbers together
number1 = 7
number2 = 8
print ("Adding Numbers")
print(number1+number2)
print()

